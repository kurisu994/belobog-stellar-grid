  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kurisu994/belobog-stellar-grid",
    "directory": "packages/worker"
  },
  "peerDependencies": {
    "belobog-stellar-grid": ">=1.0.0"
  },
  "dependencies": {
    "@bsg-export/types": "file:../types"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
